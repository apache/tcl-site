    <? prevnext "<h1>Examples</h1>" ?>

    <pre>
set seconds [clock seconds]
puts "The time is [clock format $seconds]"

proc greeting {lang} {
    switch $lang {
	en {
	    return "Hello, how is it going?"
	}
	it {
	    return "Buon giorno, come va?"
	}
	fr {
	    return "Bonjour, .......?"
	}
	de {
	    return "Guten Tag, wie gehts?"
	}
	zt {
	    return "Gr&uuml;ezi, wi gaat's?"
	}
    }
}
    </pre>

    <?footer?>
